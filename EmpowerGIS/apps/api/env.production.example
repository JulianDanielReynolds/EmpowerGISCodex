NODE_ENV=production
PORT=4000

DATABASE_URL=postgresql://username:password@host:5432/database

JWT_ACCESS_SECRET=replace-with-a-long-random-secret
JWT_ACCESS_TTL_MINUTES=15
REFRESH_TOKEN_TTL_DAYS=14
BCRYPT_ROUNDS=12

# Comma-separated
CORS_ORIGINS=https://www.empowergis.com,https://empowergis.com

# Optional, defaults to https://tiles.empowergis.com/tiles in production if unset.
TILE_BASE_URL=https://tiles.empowergis.com/tiles
TILE_MAX_FEATURES=10000
